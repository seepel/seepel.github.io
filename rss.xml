---
layout: nil
---
<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>seanplynch.com/blog</title>
    <link>http://seanplynch.com/blog/</link>
    <atom:link href="http://seanplynch.com/rss.xml" rel="self" type="application/rss+xml" />
    <description>Your Website</description>
    <language>en-us</language>
    <pubDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
    <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>

    {% for post in site.posts limit:10 %}
    <item>
      <title>{{ post.title }}</title>
      <link>http://seanplynch.com{{ post.url }}</link>
      <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
      <author>sean@seanplynch.com (Sean Lynch)</author>
      <guid>http://seanplynch.com{{ post.id }}</guid>
      <description><h2>{{ post.title | xml_escape }}</h2>{{ post.content | xml_escape }}</description>
    </item>
    {% endfor %}

  </channel> 
</rss>
